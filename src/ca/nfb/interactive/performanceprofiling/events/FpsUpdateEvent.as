package ca.nfb.interactive.performanceprofiling.events{	import flash.events.Event;	public class FpsUpdateEvent extends Event	{		public static const FPS_UPDATED:String = "FPS_UPDATED";				public var averageFPS:uint;				public function FpsUpdateEvent(type:String, averageFPS:Number, bubbles:Boolean=false, cancelable:Boolean=false)		{			this.averageFPS=averageFPS;						super(type, bubbles, cancelable);		}			}}